{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "camelcase": "off",
        "function-paren-newline": "off",
        "linebreak-style": "off",
        "no-console": "off",
        "no-underscore-dangle": "off"
    }
}
